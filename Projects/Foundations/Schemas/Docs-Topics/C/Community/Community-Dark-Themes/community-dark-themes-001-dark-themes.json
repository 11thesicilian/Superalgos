            "text": "{\n    \"backgroundColor\": \"UI_COLOR.BLACK\",\n    \"panelBackgroundColor\": \"UI_COLOR.ULTRAMARINE_BLUE\",\n    \"panelBackgroundOpacity\": \"0.1\",\n    \"panelNameColor\": \"UI_COLOR.TRUE_ORANGE\",\n    \"indicatorPanelBackgroundColor\": \"UI_COLOR.GREEN\",\n    \"timeMachineNameColor\": \"UI_COLOR.DARK\",\n    \"machineFrameColor\": \"UI_COLOR.RED\",\n    \"machineFrameOpacity\": \"0.1\",\n    \"timeScalePanelColor\": \"UI_COLOR.BLACK\",\n    \"rateScalePanelColor\": \"UI_COLOR.BLACK\",\n    \"timeFrameScaleColor\": \"UI_COLOR.BLACK\",\n    \"indicatorDropDownPanelColor\": \"UI_COLOR.GREEN\",\n    \"indicatorDropDownPanelOpacity\": \"0\",\n    \"indicatorLayersPanelColor\": \"UI_COLOR.DARK\",\n    \"indicatorLayersPanelOpacity\": \"0.1\",\n    \"indicatorLayersPanelBorderColor\": \"UI_COLOR.GREEN\",\n    \"indicatorFrameBackgroundColor\": \"UI_COLOR.RED\",\n    \"indicatorFrameBackgroundOpacity\": \"0.1\",\n    \"indicatorFrameTextColor\": \"UI_COLOR.GREEN\",\n    \"panelExchangeMarketName\": \"UI_COLOR.GREY\"\n}",
